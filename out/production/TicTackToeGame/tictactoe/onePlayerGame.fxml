<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane fx:id="root" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.OnePlayerGame">
    <children>
        <GridPane fx:id="gameGrid" hgap="10.0" layoutX="300.0" layoutY="200.0" prefHeight="200.0" prefWidth="200.0" vgap="10.0" AnchorPane.rightAnchor="300.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button fx:id="button00" mnemonicParsing="false" text="Button" />
                <Button fx:id="button01" mnemonicParsing="false" text="Button" GridPane.columnIndex="1" />
                <Button fx:id="botton02" mnemonicParsing="false" text="Button" GridPane.columnIndex="2" />
                <Button fx:id="button10" mnemonicParsing="false" text="Button" GridPane.rowIndex="1" />
                <Button fx:id="button11" mnemonicParsing="false" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Button fx:id="button12" mnemonicParsing="false" text="Button" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                <Button fx:id="button20" mnemonicParsing="false" text="Button" GridPane.rowIndex="2" />
                <Button fx:id="button21" mnemonicParsing="false" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <Button fx:id="button22" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" text="Button" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            </children>
        </GridPane>
    </children>
</AnchorPane>
